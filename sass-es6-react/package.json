{
  "name": "webpack2-es6-typescript",
  "version": "1.0.0",
  "description": "Boilerplate for Webpack 2, ES6, and React.",
  "main": "index.js",
  "scripts": {
    "dev:hot": "webpack-dev-server --config ./webpack/config.hot.js --inline --hot --no-info",
    "dev:watch": "webpack --watch --config ./webpack/config.dev.js --progress --colors",
    "dev": "webpack --config ./webpack/config.dev.js --progress --colors",
    "build": "webpack --config ./webpack/config.prod.js --progress --colors --optimize-minimize"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-airbnb": "^1.1.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "node-sass": "^4.5.0",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.13.1",
    "uglifyjs-webpack-plugin": "^0.1.4",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "license": "ISC",
  "author": "Dan Carr"
}

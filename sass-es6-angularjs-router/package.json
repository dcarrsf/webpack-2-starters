{
  "name": "webpack2-es6-angularjs-router",
  "version": "1.0.0",
  "description": "Boilerplate for Webpack 2, ES6 and AngularJS.",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=hotreloading node express.js",
    "start:dev": "npm run dev && NODE_ENV=development node express.js",
    "start:prod": "npm run build && NODE_ENV=production node express.js",
    "dev:watch": "webpack --watch --config ./webpack/config.dev.js --progress --colors",
    "dev": "webpack --config ./webpack/config.dev.js --progress --colors",
    "build": "webpack --config ./webpack/config.prod.js --progress --colors --optimize-minimize"
  },
  "dependencies": {
    "angular": "^1.6.2",
    "angular-route": "^1.6.2"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.13.1",
    "uglifyjs-webpack-plugin": "^0.1.4",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.17.0"
  },
  "license": "ISC",
  "author": "Dan Carr"
}
